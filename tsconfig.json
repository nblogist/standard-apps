{
  "extends": "@polkadot/dev/config/tsconfig",
  "exclude": ["build/**/*", "**/build/**/*"],
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@canvas-ui/apps/*": ["packages/apps/src/*"],
      "@canvas-ui/apps": ["packages/apps/src"],
      "@canvas-ui/apps-config/*": ["packages/apps-config/src/*"],
      "@canvas-ui/apps-routing": ["packages/apps-routing/src"],
      "@canvas-ui/apps-routing/*": ["packages/apps-routing/src/*"],
      "@canvas-ui/app-deploy": ["packages/page-deploy/src"],
      "@canvas-ui/app-deploy/*": ["packages/page-deploy/src/*"],
      "@canvas-ui/app-execute": ["packages/page-execute/src"],
      "@canvas-ui/app-execute/*": ["packages/page-execute/src/*"],
      "@canvas-ui/app-i18n": ["packages/page-i18n/src"],
      "@canvas-ui/app-i18n/*": ["packages/page-i18n/src/*"],
      "@canvas-ui/app-settings": ["packages/page-settings/src"],
      "@canvas-ui/app-settings/*": ["packages/page-settings/src/*"],
      "@canvas-ui/app-upload": ["packages/page-upload/src"],
      "@canvas-ui/app-upload/*": ["packages/page-upload/src/*"],
      "@canvas-ui/app-swap": ["packages/page-swap/src"],
      "@canvas-ui/app-swap/*": ["packages/page-swap/src/*"],
      "@canvas-ui/app-farm": ["packages/page-farm/src"],
      "@canvas-ui/app-farm/*": ["packages/page-farm/src/*"],
      "@canvas-ui/app-home": ["packages/page-home/src"],
      "@canvas-ui/app-home/*": ["packages/page-home/src/*"],
      "@canvas-ui/app-vault": ["packages/page-vault/src"],
      "@canvas-ui/app-vault/*": ["packages/page-vault/src/*"],
      "@canvas-ui/app-collateralize": ["packages/page-collateralize/src"],
      "@canvas-ui/app-collateralize/*": ["packages/page-collateralize/src/*"],
      "@canvas-ui/custom-components": ["packages/custom-components/src"],
      "@canvas-ui/react-api": ["packages/react-api/src"],
      "@canvas-ui/react-api/*": ["packages/react-api/src/*"],
      "@canvas-ui/react-components": ["packages/react-components/src"],
      "@canvas-ui/react-components/*": ["packages/react-components/src/*"],
      "@canvas-ui/react-hooks": ["packages/react-hooks/src"],
      "@canvas-ui/react-hooks/*": ["packages/react-hooks/src/*"],
      "@canvas-ui/react-params": ["packages/react-params/src"],
      "@canvas-ui/react-params/*": ["packages/react-params/src/*"],
      "@canvas-ui/react-query": ["packages/react-query/src"],
      "@canvas-ui/react-query/*": ["packages/react-query/src/*"],
      "@canvas-ui/react-signer": ["packages/react-signer/src"],
      "@canvas-ui/react-signer/*": ["packages/react-signer/src/*"],
      "@canvas-ui/react-util": ["packages/react-util/src"],
      "@canvas-ui/react-util/*": ["packages/react-util/src/*"],
      "@canvas-ui/test-support/*": ["packages/test-support/src/*"],
      "@canvas-ui/test-support": ["packages/test-support/src"],
      "@polkadot/types/*": ["packages/types/src/*"],
      "@polkadot/types": ["packages/types/src"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@polkadot/ts", "./node_modules/@types"]
  }
}
